{
  "name": "Lova Tech Innovation Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#FFFFFF",
    "variable.readonly": "#FFFFFF",
    "variable.local": "#FFFFFF",
    "variable.declaration": "#FFFFFF",
    "property": "#FFFFFF",
    "parameter": "#ffcc00"
  },
  "colors": {
    "activityBar.activeBorder": "#00ffff",
    "activityBar.background": "#1e1e1e",
    "activityBar.border": "#151515",
    "activityBar.foreground": "#00ffff",
    "activityBar.inactiveForeground": "#6080a0",
    "activityBarBadge.background": "#0066ff",
    "activityBarBadge.foreground": "#ffffff",
    "editor.background": "#1e1e1e",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#252535",
    "editor.selectionBackground": "#0066ff40",
    "editor.wordHighlightBackground": "#0066ff30",
    "editor.findMatchBackground": "#0066ff50",
    "editor.findMatchHighlightBackground": "#0066ff30",
    "editorCursor.foreground": "#00ffff",
    "editorGroupHeader.tabsBackground": "#151520",
    "editorLineNumber.foreground": "#4a5568",
    "editorLineNumber.activeForeground": "#00ffff",
    "editorIndentGuide.background": "#2d2d3d",
    "editorIndentGuide.activeBackground": "#3d3d5d",
    "editorBracketMatch.background": "#0066ff40",
    "editorBracketMatch.border": "#0066ff",
    "panel.background": "#151520",
    "panel.border": "#2d2d3d",
    "panelTitle.activeForeground": "#00ffff",
    "panelTitle.activeBorder": "#0066ff",
    "panelTitle.inactiveForeground": "#6080a0",
    "panelSectionHeader.background": "#1e1e1e",
    "sideBar.background": "#151520",
    "sideBar.border": "#2d2d3d",
    "sideBar.foreground": "#c0c0d0",
    "sideBarSectionHeader.background": "#1e1e1e",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarTitle.foreground": "#0066ff",
    "statusBar.background": "#0066ff",
    "statusBar.border": "#0055dd",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingBackground": "#ff4444",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#1e1e1e",
    "tab.activeBackground": "#1e1e1e",
    "tab.activeForeground": "#ffffff",
    "tab.activeBorderTop": "#0066ff",
    "tab.inactiveBackground": "#151520",
    "tab.inactiveForeground": "#6080a0",
    "tab.border": "#15152080",
    "terminal.background": "#1e1e1e",
    "terminal.foreground": "#ffffff",
    "terminalCursor.foreground": "#00ffff",
    "terminal.ansiBlack": "#1e1e1e",
    "terminal.ansiRed": "#ff4444",
    "terminal.ansiGreen": "#00ff88",
    "terminal.ansiYellow": "#ffcc00",
    "terminal.ansiBlue": "#0066ff",
    "terminal.ansiMagenta": "#cc66ff",
    "terminal.ansiCyan": "#00ffff",
    "terminal.ansiWhite": "#ffffff",
    "titleBar.activeBackground": "#151520",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#151520",
    "titleBar.inactiveForeground": "#6080a0",
    "titleBar.border": "#2d2d3d",
    "quickInput.background": "#1e1e1e",
    "quickInput.foreground": "#ffffff",
    "quickInputList.focusBackground": "#0066ff40",
    "quickInputList.focusForeground": "#ffffff",
    "quickInputList.focusIconForeground": "#00ffff",
    "quickInputTitle.background": "#1e1e1e",
    "pickerGroup.border": "#2d2d3d",
    "pickerGroup.foreground": "#0066ff",
    "list.activeSelectionBackground": "#0066ff40",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#252535",
    "list.inactiveSelectionBackground": "#2d2d3d",
    "scrollbarSlider.background": "#0066ff40",
    "scrollbarSlider.hoverBackground": "#0066ff60",
    "scrollbarSlider.activeBackground": "#0066ff80",
    "input.background": "#151520",
    "input.border": "#2d2d3d",
    "input.foreground": "#ffffff",
    "input.placeholderForeground": "#6080a0",
    "inputOption.activeBorder": "#0066ff",
    "button.background": "#0066ff",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#0077ff",
    "dropdown.background": "#151520",
    "dropdown.border": "#2d2d3d",
    "dropdown.foreground": "#ffffff",
    "badge.background": "#0066ff",
    "badge.foreground": "#ffffff",
    "progressBar.background": "#0066ff",
    "gitDecoration.modifiedResourceForeground": "#ffcc00",
    "gitDecoration.deletedResourceForeground": "#ff4444",
    "gitDecoration.untrackedResourceForeground": "#00ff88",
    "gitDecoration.addedResourceForeground": "#00ff88",
    "minimap.selectionHighlight": "#0066ff50"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6080a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Template String",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ff4444"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#ff4444"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#0066ff"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#cc66ff"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.function.parameters"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Variable Constant",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#0066ff"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#c0c0d0"
      }
    },
    {
      "name": "Punctuation Bracket",
      "scope": ["punctuation.definition.block", "punctuation.brackets"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff4444"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#0066ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "JSON Value String",
      "scope": ["meta.structure.dictionary.value.json string.quoted"],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "CSS Unit",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#ff4444"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#cc66ff"
      }
    },
    {
      "name": "Escape Character",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cc66ff"
      }
    },
    {
      "name": "Decorator",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#cc66ff"
      }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "foreground": "#ff4444",
        "fontStyle": "italic"
      }
    }
  ]
}
