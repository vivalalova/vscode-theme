    "terminal.ansiBlue": "#7090a0",
    "terminal.ansiMagenta": "#a08090",
    "terminal.ansiCyan": "#7d8471",
    "terminal.ansiWhite": "#faf9f6",
    "titleBar.activeBackground": "#243d22",
    "titleBar.activeForeground": "#faf9f6",
    "titleBar.inactiveBackground": "#243d22",
