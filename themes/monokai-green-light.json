{
  "name": "Lova Monokai Green Light",
  "type": "light",
  "colors": {
    "activityBar.activeBorder": "#1a9e84",
    "activityBar.background": "#e8e8e8",
    "activityBar.foreground": "#1a9e84",
    "activityBar.inactiveForeground": "#666666",
    "activityBarBadge.background": "#1a9e84",
    "activityBarBadge.foreground": "#ffffff",
    "editor.background": "#f0f0f0",
    "editorGroupHeader.tabsBackground": "#e5e5e5",
    "panel.background": "#f0f0f0",
    "panel.border": "#d5d5d5",
    "panelTitle.activeForeground": "#1a9e84",
    "panelTitle.inactiveForeground": "#666666",
    "sideBar.background": "#e8e8e8",
    "sideBarSectionHeader.background": "#dedede",
    "sideBarSectionHeader.foreground": "#333333",
    "sideBarTitle.foreground": "#1a9e84",
    "statusBar.background": "#1a9e84",
    "statusBar.debuggingBackground": "#cc6633",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ffffff",
    "tab.activeBackground": "#f0f0f0",
    "tab.activeForeground": "#333333",
    "tab.inactiveBackground": "#e0e0e0",
    "tab.inactiveForeground": "#666666",
    "terminal.background": "#f0f0f0",
    "terminal.foreground": "#333333",
    "terminalCursor.foreground": "#1a9e84",
    "titleBar.activeBackground": "#dedede",
    "titleBar.activeForeground": "#333333",
    "titleBar.inactiveBackground": "#dedede",
    "titleBar.inactiveForeground": "#666666",
    "quickInput.background": "#f0f0f0",
    "quickInput.foreground": "#333333",
    "quickInputList.focusBackground": "#d8ebe7",
    "quickInputList.focusForeground": "#333333",
    "quickInputList.focusIconForeground": "#1a9e84",
    "quickInputTitle.background": "#f0f0f0",
    "pickerGroup.border": "#d5d5d5",
    "pickerGroup.foreground": "#1a9e84"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7d7d60",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#6b6b00"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#7050b0"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#7050b0"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#558018"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#558018",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#188a75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#b86018",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#558018"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#c41868"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#558018",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#188a75",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#6b6b00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#b86018"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    }
  ]
}
