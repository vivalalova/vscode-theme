{
  "name": "Lova Monokai Green Light",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#1a1a1a",
    "variable.readonly": "#1a1a1a",
    "variable.local": "#1a1a1a",
    "variable.declaration": "#1a1a1a",
    "property": "#1a1a1a",
    "parameter": "#b86018"
  },
  "colors": {
    "activityBar.activeBorder": "#1a9e84",
    "activityBar.background": "#eeeeee",
    "activityBar.border": "#d0d0d0",
    "activityBar.foreground": "#1a9e84",
    "activityBar.inactiveForeground": "#666666",
    "activityBarBadge.background": "#1a9e84",
    "activityBarBadge.foreground": "#ffffff",
    "contrastBorder": "#d0d0d0",
    "editor.background": "#f8f8f8",
    "editorGroup.border": "#d0d0d0",
    "editorGroupHeader.border": "#d0d0d0",
    "editorGroupHeader.tabsBorder": "#d0d0d0",
    "editorGroupHeader.tabsBackground": "#eeeeee",
    "panel.background": "#f8f8f8",
    "panel.border": "#d0d0d0",
    "panelSection.border": "#d0d0d0",
    "panelSectionHeader.border": "#d0d0d0",
    "panelTitle.activeBorder": "#1a9e84",
    "panelTitle.activeForeground": "#1a9e84",
    "panelTitle.inactiveForeground": "#666666",
    "sideBar.background": "#f0f0f0",
    "sideBar.border": "#d0d0d0",
    "sideBarSectionHeader.background": "#e8e8e8",
    "sideBarSectionHeader.border": "#d0d0d0",
    "sideBarSectionHeader.foreground": "#333333",
    "sideBarTitle.foreground": "#1a9e84",
    "statusBar.background": "#1a9e84",
    "statusBar.border": "#158a72",
    "statusBar.debuggingBackground": "#cc6633",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ffffff",
    "tab.activeBackground": "#f8f8f8",
    "tab.activeForeground": "#333333",
    "tab.border": "#d0d0d0",
    "tab.inactiveBackground": "#e8e8e8",
    "tab.inactiveForeground": "#666666",
    "terminal.background": "#f8f8f8",
    "terminal.border": "#d0d0d0",
    "terminal.foreground": "#333333",
    "terminalCursor.foreground": "#1a9e84",
    "titleBar.activeBackground": "#e8e8e8",
    "titleBar.activeForeground": "#333333",
    "titleBar.border": "#d0d0d0",
    "titleBar.inactiveBackground": "#e8e8e8",
    "titleBar.inactiveForeground": "#666666",
    "quickInput.background": "#f8f8f8",
    "quickInput.foreground": "#333333",
    "quickInputList.focusBackground": "#e0f0ec",
    "quickInputList.focusForeground": "#333333",
    "quickInputList.focusIconForeground": "#1a9e84",
    "quickInputTitle.background": "#f8f8f8",
    "pickerGroup.border": "#d0d0d0",
    "pickerGroup.foreground": "#1a9e84",
    "widget.border": "#d0d0d0",
    "input.border": "#d0d0d0",
    "inputOption.activeBorder": "#1a9e84",
    "dropdown.border": "#d0d0d0",
    "list.activeSelectionBackground": "#d0e8e3",
    "list.activeSelectionForeground": "#1a1a1a",
    "list.focusForeground": "#1a1a1a",
    "list.hoverBackground": "#e8e8e8",
    "list.inactiveSelectionForeground": "#1a1a1a",
    "sideBar.foreground": "#1a1a1a",
    "gitDecoration.modifiedResourceForeground": "#c08000",
    "gitDecoration.deletedResourceForeground": "#d02020",
    "gitDecoration.untrackedResourceForeground": "#2a8a2a",
    "gitDecoration.conflictingResourceForeground": "#d02020",
    "gitDecoration.stageModifiedResourceForeground": "#c08000",
    "gitDecoration.stageDeletedResourceForeground": "#d02020"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7d7d60",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#6b6b00"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#7050b0"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#7050b0"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#558018"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#558018",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#188a75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#b86018",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#c41868"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#558018"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#c41868"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#558018",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#188a75",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#6b6b00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#b86018"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#188a75"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#2a2a2a"
      }
    }
  ]
}
