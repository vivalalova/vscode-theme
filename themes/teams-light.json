{
  "name": "Lova Teams Light",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#1a1a1a",
    "variable.readonly": "#1a1a1a",
    "variable.local": "#1a1a1a",
    "variable.declaration": "#1a1a1a",
    "property": "#1a1a1a",
    "parameter": "#323130"
  },
  "colors": {
    "activityBar.activeBorder": "#6264A7",
    "activityBar.background": "#F3F2F1",
    "activityBar.foreground": "#6264A7",
    "activityBar.inactiveForeground": "#616161",
    "activityBarBadge.background": "#6264A7",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#242424",
    "editor.lineHighlightBackground": "#F5F5F5",
    "editor.selectionBackground": "#6264A730",
    "editorCursor.foreground": "#6264A7",
    "editorGroupHeader.tabsBackground": "#F3F2F1",
    "editorIndentGuide.activeBackground1": "#C8C6C4",
    "editorIndentGuide.background1": "#E1DFDD",
    "editorLineNumber.activeForeground": "#6264A7",
    "editorLineNumber.foreground": "#A19F9D",
    "focusBorder": "#6264A7",
    "input.background": "#FFFFFF",
    "input.border": "#D2D0CE",
    "input.foreground": "#242424",
    "input.placeholderForeground": "#8A8886",
    "list.activeSelectionBackground": "#E8E8F4",
    "list.activeSelectionForeground": "#242424",
    "list.hoverBackground": "#F5F5F5",
    "panel.background": "#F3F2F1",
    "panel.border": "#E1DFDD",
    "panelTitle.activeForeground": "#6264A7",
    "panelTitle.inactiveForeground": "#616161",
    "sideBar.background": "#F3F2F1",
    "sideBar.foreground": "#323130",
    "sideBarSectionHeader.background": "#E1DFDD",
    "sideBarSectionHeader.foreground": "#323130",
    "sideBarTitle.foreground": "#6264A7",
    "statusBar.background": "#6264A7",
    "statusBar.debuggingBackground": "#5059C9",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#D2D0CE",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeBorderTop": "#6264A7",
    "tab.activeForeground": "#242424",
    "tab.inactiveBackground": "#F3F2F1",
    "tab.inactiveForeground": "#605E5C",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#242424",
    "terminalCursor.foreground": "#6264A7",
    "titleBar.activeBackground": "#F3F2F1",
    "titleBar.activeForeground": "#323130",
    "titleBar.inactiveBackground": "#F3F2F1",
    "titleBar.inactiveForeground": "#8A8886",
    "quickInput.background": "#F3F2F1",
    "quickInput.foreground": "#242424",
    "quickInputList.focusBackground": "#E8E8F4",
    "quickInputList.focusForeground": "#242424",
    "quickInputList.focusIconForeground": "#6264A7",
    "quickInputTitle.background": "#F3F2F1",
    "pickerGroup.border": "#E1DFDD",
    "pickerGroup.foreground": "#6264A7",
    "button.background": "#6264A7",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#5059C9",
    "scrollbarSlider.activeBackground": "#6264A780",
    "scrollbarSlider.background": "#C8C6C450",
    "scrollbarSlider.hoverBackground": "#A19F9D70",
    "badge.background": "#6264A7",
    "badge.foreground": "#FFFFFF",
    "dropdown.background": "#FFFFFF",
    "dropdown.border": "#D2D0CE",
    "dropdown.foreground": "#242424",
    "dropdown.listBackground": "#FFFFFF",
    "editorWidget.background": "#F3F2F1",
    "editorWidget.border": "#E1DFDD",
    "editorWidget.foreground": "#242424",
    "editorHoverWidget.background": "#F3F2F1",
    "editorHoverWidget.border": "#E1DFDD",
    "editorSuggestWidget.background": "#F3F2F1",
    "editorSuggestWidget.border": "#E1DFDD",
    "editorSuggestWidget.selectedBackground": "#E8E8F4",
    "peekView.border": "#6264A7",
    "peekViewEditor.background": "#F3F2F1",
    "peekViewResult.background": "#F5F5F5",
    "peekViewTitle.background": "#F5F5F5",
    "notificationCenterHeader.background": "#F3F2F1",
    "notifications.background": "#FFFFFF",
    "notifications.border": "#E1DFDD",
    "progressBar.background": "#6264A7",
    "textLink.foreground": "#6264A7",
    "textLink.activeForeground": "#5059C9",
    "gitDecoration.addedResourceForeground": "#13A40E",
    "gitDecoration.modifiedResourceForeground": "#5059C9",
    "gitDecoration.deletedResourceForeground": "#C4314B",
    "gitDecoration.untrackedResourceForeground": "#13A40E",
    "debugToolBar.background": "#F3F2F1",
    "debugIcon.breakpointForeground": "#C4314B",
    "debugIcon.startForeground": "#13A40E"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8A8886",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#C4314B"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#13A40E"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#5059C9"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#6264A7"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#323130"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#8E562E"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#3D45AF"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#3D45AF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#323130"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#323130",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#5059C9"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C4314B"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#6264A7"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#5059C9"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#323130"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#C4314B"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#6264A7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#323130",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#323130",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#6264A7"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#C4314B"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#6264A7"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#323130"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#323130"
      }
    }
  ]
}
