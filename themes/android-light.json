{
  "name": "Lova Android Light",
  "type": "light",
  "colors": {
    "activityBar.activeBorder": "#1A73E8",
    "activityBar.background": "#FEFBFF",
    "activityBar.foreground": "#1A73E8",
    "activityBar.inactiveForeground": "#49454F",
    "activityBarBadge.background": "#1A73E8",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#FFFBFE",
    "editor.foreground": "#1C1B1F",
    "editor.lineHighlightBackground": "#F7F2FA",
    "editor.selectionBackground": "#1A73E830",
    "editorCursor.foreground": "#1A73E8",
    "editorGroupHeader.tabsBackground": "#F3EDF7",
    "editorIndentGuide.activeBackground1": "#CAC4D0",
    "editorIndentGuide.background1": "#E7E0EC",
    "editorLineNumber.activeForeground": "#1A73E8",
    "editorLineNumber.foreground": "#79747E",
    "focusBorder": "#1A73E8",
    "input.background": "#FFFBFE",
    "input.border": "#CAC4D0",
    "input.foreground": "#1C1B1F",
    "input.placeholderForeground": "#79747E",
    "list.activeSelectionBackground": "#1A73E820",
    "list.activeSelectionForeground": "#1C1B1F",
    "list.hoverBackground": "#F3EDF7",
    "panel.background": "#FEFBFF",
    "panel.border": "#CAC4D0",
    "panelTitle.activeForeground": "#1A73E8",
    "panelTitle.inactiveForeground": "#79747E",
    "sideBar.background": "#FEFBFF",
    "sideBar.foreground": "#1C1B1F",
    "sideBarSectionHeader.background": "#E7E0EC",
    "sideBarSectionHeader.foreground": "#1C1B1F",
    "sideBarTitle.foreground": "#1A73E8",
    "statusBar.background": "#1A73E8",
    "statusBar.debuggingBackground": "#EA4335",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#CAC4D0",
    "tab.activeBackground": "#FFFBFE",
    "tab.activeBorderTop": "#1A73E8",
    "tab.activeForeground": "#1C1B1F",
    "tab.inactiveBackground": "#F3EDF7",
    "tab.inactiveForeground": "#79747E",
    "terminal.background": "#FFFBFE",
    "terminal.foreground": "#1C1B1F",
    "terminalCursor.foreground": "#1A73E8",
    "titleBar.activeBackground": "#F3EDF7",
    "titleBar.activeForeground": "#1C1B1F",
    "titleBar.inactiveBackground": "#F3EDF7",
    "titleBar.inactiveForeground": "#79747E",
    "quickInput.background": "#FEFBFF",
    "quickInput.foreground": "#1C1B1F",
    "quickInputList.focusBackground": "#E7E0EC",
    "quickInputList.focusForeground": "#1C1B1F",
    "quickInputList.focusIconForeground": "#1A73E8",
    "quickInputTitle.background": "#FEFBFF",
    "pickerGroup.border": "#CAC4D0",
    "pickerGroup.foreground": "#1A73E8",
    "button.background": "#1A73E8",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#1557B0",
    "scrollbarSlider.activeBackground": "#1A73E880",
    "scrollbarSlider.background": "#79747E40",
    "scrollbarSlider.hoverBackground": "#79747E60"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#79747E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#34A853"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#1A73E8"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#1A73E8"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#9334E6"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#185ABC",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#185ABC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9334E6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#1A73E8"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#185ABC"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#FBBC04"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#EA4335"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#1A73E8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#1A73E8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#34A853",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1A73E8"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#34A853"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "punctuation.support.type.property-name.begin.json", "punctuation.support.type.property-name.end.json"],
      "settings": {
        "foreground": "#185ABC"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#1C1B1F"
      }
    }
  ]
}
