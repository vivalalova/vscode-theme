{
  "name": "Lova Claude Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#FFFFFF",
    "variable.readonly": "#FFFFFF",
    "variable.local": "#FFFFFF",
    "variable.declaration": "#FFFFFF",
    "property": "#FFFFFF",
    "parameter": "#FCD34D"
  },
  "colors": {
    "activityBar.activeBorder": "#D97757",
    "activityBar.background": "#1A1918",
    "activityBar.foreground": "#D97757",
    "activityBar.inactiveForeground": "#7A7570",
    "activityBarBadge.background": "#D97757",
    "activityBarBadge.foreground": "#131314",
    "editor.background": "#131314",
    "editor.foreground": "#E5E3DE",
    "editor.lineHighlightBackground": "#1E1D1C",
    "editor.selectionBackground": "#D9775740",
    "editorCursor.foreground": "#D97757",
    "editorGroupHeader.tabsBackground": "#1A1918",
    "editorIndentGuide.activeBackground1": "#3A3835",
    "editorIndentGuide.background1": "#252422",
    "editorLineNumber.activeForeground": "#D97757",
    "editorLineNumber.foreground": "#5A5755",
    "focusBorder": "#D97757",
    "input.background": "#1A1918",
    "input.border": "#2E2C2A",
    "input.foreground": "#E5E3DE",
    "input.placeholderForeground": "#6A6765",
    "list.activeSelectionBackground": "#D9775730",
    "list.activeSelectionForeground": "#E5E3DE",
    "list.hoverBackground": "#1E1D1C",
    "panel.background": "#131314",
    "panel.border": "#2E2C2A",
    "panelTitle.activeForeground": "#D97757",
    "panelTitle.inactiveForeground": "#7A7570",
    "sideBar.background": "#1A1918",
    "sideBar.foreground": "#C5C2BD",
    "sideBarSectionHeader.background": "#1E1D1C",
    "sideBarSectionHeader.foreground": "#C5C2BD",
    "sideBarTitle.foreground": "#D97757",
    "statusBar.background": "#D97757",
    "statusBar.debuggingBackground": "#B85A40",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#131314",
    "statusBar.noFolderBackground": "#2E2C2A",
    "tab.activeBackground": "#131314",
    "tab.activeBorderTop": "#D97757",
    "tab.activeForeground": "#E5E3DE",
    "tab.inactiveBackground": "#1A1918",
    "tab.inactiveForeground": "#7A7570",
    "terminal.background": "#131314",
    "terminal.foreground": "#E5E3DE",
    "terminalCursor.foreground": "#D97757",
    "titleBar.activeBackground": "#1A1918",
    "titleBar.activeForeground": "#C5C2BD",
    "titleBar.inactiveBackground": "#1A1918",
    "titleBar.inactiveForeground": "#5A5755",
    "quickInput.background": "#1A1918",
    "quickInput.foreground": "#E5E3DE",
    "quickInputList.focusBackground": "#252422",
    "quickInputList.focusForeground": "#E5E3DE",
    "quickInputList.focusIconForeground": "#D97757",
    "quickInputTitle.background": "#1A1918",
    "pickerGroup.border": "#2E2C2A",
    "pickerGroup.foreground": "#D97757",
    "button.background": "#D97757",
    "button.foreground": "#131314",
    "button.hoverBackground": "#E8866A",
    "scrollbarSlider.activeBackground": "#D9775780",
    "scrollbarSlider.background": "#3A383560",
    "scrollbarSlider.hoverBackground": "#4A474580"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A6765",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#6EE7B7"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#C4B5FD"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#C4B5FD"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#A8A5A0"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#D8B4FE",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#67E8F9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#E5E3DE"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FCD34D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#C5C2BD"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#67E8F9"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#A8A5A0"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E5E3DE",
        "background": "#DC2626"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#E5E3DE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#C5C2BD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#93C5FD"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#E5E3DE"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#E5E3DE"
      }
    }
  ]
}
