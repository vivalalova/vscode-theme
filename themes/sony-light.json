{
  "name": "Lova Sony Light",
  "type": "light",
  "colors": {
    "activityBar.activeBorder": "#003791",
    "activityBar.background": "#F7F7F7",
    "activityBar.foreground": "#003791",
    "activityBar.inactiveForeground": "#5C5C5C",
    "activityBarBadge.background": "#003791",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#121212",
    "editor.lineHighlightBackground": "#F7F7F7",
    "editor.selectionBackground": "#00379128",
    "editorCursor.foreground": "#003791",
    "editorGroupHeader.tabsBackground": "#F0F0F0",
    "editorIndentGuide.activeBackground1": "#BFBFBF",
    "editorIndentGuide.background1": "#DEDEDE",
    "editorLineNumber.activeForeground": "#003791",
    "editorLineNumber.foreground": "#5C5C5C",
    "focusBorder": "#003791",
    "input.background": "#FFFFFF",
    "input.border": "#BFBFBF",
    "input.foreground": "#121212",
    "input.placeholderForeground": "#5C5C5C",
    "list.activeSelectionBackground": "#00379118",
    "list.activeSelectionForeground": "#121212",
    "list.hoverBackground": "#F0F0F0",
    "panel.background": "#F7F7F7",
    "panel.border": "#BFBFBF",
    "panelTitle.activeForeground": "#003791",
    "panelTitle.inactiveForeground": "#5C5C5C",
    "sideBar.background": "#F7F7F7",
    "sideBar.foreground": "#121212",
    "sideBarSectionHeader.background": "#DEDEDE",
    "sideBarSectionHeader.foreground": "#121212",
    "sideBarTitle.foreground": "#003791",
    "statusBar.background": "#003791",
    "statusBar.debuggingBackground": "#0070C9",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#BFBFBF",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeBorderTop": "#003791",
    "tab.activeForeground": "#121212",
    "tab.inactiveBackground": "#F0F0F0",
    "tab.inactiveForeground": "#5C5C5C",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#121212",
    "terminalCursor.foreground": "#003791",
    "titleBar.activeBackground": "#F0F0F0",
    "titleBar.activeForeground": "#121212",
    "titleBar.inactiveBackground": "#F0F0F0",
    "titleBar.inactiveForeground": "#5C5C5C",
    "quickInput.background": "#F7F7F7",
    "quickInput.foreground": "#121212",
    "quickInputList.focusBackground": "#DEDEDE",
    "quickInputList.focusForeground": "#121212",
    "quickInputList.focusIconForeground": "#003791",
    "quickInputTitle.background": "#F7F7F7",
    "pickerGroup.border": "#BFBFBF",
    "pickerGroup.foreground": "#003791",
    "button.background": "#003791",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#002B70",
    "scrollbarSlider.activeBackground": "#00379180",
    "scrollbarSlider.background": "#5C5C5C40",
    "scrollbarSlider.hoverBackground": "#5C5C5C60"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5C5C5C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#0070C9"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#003791"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#6B3FA0"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#003791"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#121212"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#6B3FA0"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#004A98",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#004A98",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#121212"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#6B3FA0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#121212"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#003791"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#004A98"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#0070C9"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#121212"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#D32F2F"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#003791",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#6B3FA0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#0070C9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#003791"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#0070C9"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "punctuation.support.type.property-name.begin.json", "punctuation.support.type.property-name.end.json"],
      "settings": {
        "foreground": "#004A98"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#121212"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#121212"
      }
    }
  ]
}
