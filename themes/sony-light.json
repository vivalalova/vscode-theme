{
  "name": "Lova Sony Light",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#1a1a1a",
    "variable.readonly": "#1a1a1a",
    "variable.local": "#1a1a1a",
    "variable.declaration": "#1a1a1a",
    "property": "#1a1a1a",
    "parameter": "#6278BA"
  },
  "colors": {
    "activityBar.activeBorder": "#0070D1",
    "activityBar.background": "#F5F5F7",
    "activityBar.foreground": "#0070D1",
    "activityBar.inactiveForeground": "#526879",
    "activityBarBadge.background": "#DF0024",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#252C46",
    "editor.lineHighlightBackground": "#F5F5F7",
    "editor.selectionBackground": "#0070D128",
    "editorCursor.foreground": "#0070D1",
    "editorGroupHeader.tabsBackground": "#F0F0F2",
    "editorIndentGuide.activeBackground1": "#AEB5C6",
    "editorIndentGuide.background1": "#D8DBE4",
    "editorLineNumber.activeForeground": "#0070D1",
    "editorLineNumber.foreground": "#526879",
    "focusBorder": "#5CC9FB",
    "input.background": "#FFFFFF",
    "input.border": "#AEB5C6",
    "input.foreground": "#252C46",
    "input.placeholderForeground": "#526879",
    "list.activeSelectionBackground": "#0070D118",
    "list.activeSelectionForeground": "#252C46",
    "list.hoverBackground": "#F0F0F2",
    "panel.background": "#F5F5F7",
    "panel.border": "#AEB5C6",
    "panelTitle.activeForeground": "#0070D1",
    "panelTitle.inactiveForeground": "#526879",
    "sideBar.background": "#F5F5F7",
    "sideBar.foreground": "#252C46",
    "sideBarSectionHeader.background": "#E8E9EE",
    "sideBarSectionHeader.foreground": "#252C46",
    "sideBarTitle.foreground": "#0070D1",
    "statusBar.background": "#0070D1",
    "statusBar.debuggingBackground": "#DF0024",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#AEB5C6",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeBorderTop": "#0070D1",
    "tab.activeForeground": "#252C46",
    "tab.inactiveBackground": "#F0F0F2",
    "tab.inactiveForeground": "#526879",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#252C46",
    "terminalCursor.foreground": "#0070D1",
    "titleBar.activeBackground": "#003087",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#003087",
    "titleBar.inactiveForeground": "#AEB5C6",
    "quickInput.background": "#F5F5F7",
    "quickInput.foreground": "#252C46",
    "quickInputList.focusBackground": "#E8E9EE",
    "quickInputList.focusForeground": "#252C46",
    "quickInputList.focusIconForeground": "#0070D1",
    "quickInputTitle.background": "#F5F5F7",
    "pickerGroup.border": "#AEB5C6",
    "pickerGroup.foreground": "#0070D1",
    "button.background": "#0070D1",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#003087",
    "scrollbarSlider.activeBackground": "#0070D180",
    "scrollbarSlider.background": "#52687940",
    "scrollbarSlider.hoverBackground": "#52687960"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#526879",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#00AC9F"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DF0024"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#003087"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#252C46"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#2E6DB4"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#0070D1",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#0070D1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#252C46"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#6278BA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#404F7D"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#003087"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#0070D1"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#5CC9FB"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#252C46"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#DF0024"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#003087",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#DF0024",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#00AC9F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#0070D1"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#2E6DB4"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "punctuation.support.type.property-name.begin.json", "punctuation.support.type.property-name.end.json"],
      "settings": {
        "foreground": "#0070D1"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#252C46"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#252C46"
      }
    }
  ]
}
