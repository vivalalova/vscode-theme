{
  "name": "Lova Sony Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#FFFFFF",
    "variable.readonly": "#FFFFFF",
    "variable.local": "#FFFFFF",
    "variable.declaration": "#FFFFFF",
    "property": "#FFFFFF",
    "parameter": "#9BAFD9"
  },
  "colors": {
