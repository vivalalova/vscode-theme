{
  "name": "Lova Claude",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#1a1a1a",
    "variable.readonly": "#1a1a1a",
    "variable.local": "#1a1a1a",
    "variable.declaration": "#1a1a1a",
    "property": "#1a1a1a",
    "parameter": "#B45309"
  },
  "colors": {
    "activityBar.activeBorder": "#C15F3C",
    "activityBar.background": "#F4F3EE",
    "activityBar.foreground": "#C15F3C",
    "activityBar.inactiveForeground": "#8A857D",
    "activityBarBadge.background": "#C15F3C",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#F4F3EE",
    "editor.foreground": "#2D2A26",
    "editor.lineHighlightBackground": "#EAE9E4",
    "editor.selectionBackground": "#C15F3C30",
    "editorCursor.foreground": "#C15F3C",
    "editorGroupHeader.tabsBackground": "#ECEAE5",
    "editorIndentGuide.activeBackground1": "#C5C2BA",
    "editorIndentGuide.background1": "#DBD9D3",
    "editorLineNumber.activeForeground": "#C15F3C",
    "editorLineNumber.foreground": "#9A958D",
    "focusBorder": "#C15F3C",
    "input.background": "#FAFAF8",
    "input.border": "#D5D2CB",
    "input.foreground": "#2D2A26",
    "input.placeholderForeground": "#918C84",
    "list.activeSelectionBackground": "#C15F3C25",
    "list.activeSelectionForeground": "#2D2A26",
    "list.hoverBackground": "#ECEAE5",
    "panel.background": "#F4F3EE",
    "panel.border": "#D5D2CB",
    "panelTitle.activeForeground": "#C15F3C",
    "panelTitle.inactiveForeground": "#8A857D",
    "sideBar.background": "#ECEAE5",
    "sideBar.foreground": "#3D3935",
    "sideBarSectionHeader.background": "#E4E2DC",
    "sideBarSectionHeader.foreground": "#3D3935",
    "sideBarTitle.foreground": "#C15F3C",
    "statusBar.background": "#C15F3C",
    "statusBar.debuggingBackground": "#A04D30",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#D5D2CB",
    "tab.activeBackground": "#F4F3EE",
    "tab.activeBorderTop": "#C15F3C",
    "tab.activeForeground": "#2D2A26",
    "tab.inactiveBackground": "#ECEAE5",
    "tab.inactiveForeground": "#6A6560",
    "terminal.background": "#F4F3EE",
    "terminal.foreground": "#2D2A26",
    "terminalCursor.foreground": "#C15F3C",
    "titleBar.activeBackground": "#ECEAE5",
    "titleBar.activeForeground": "#3D3935",
    "titleBar.inactiveBackground": "#ECEAE5",
    "titleBar.inactiveForeground": "#8A857D",
    "quickInput.background": "#F4F3EE",
    "quickInput.foreground": "#2D2A26",
    "quickInputList.focusBackground": "#E4E2DC",
    "quickInputList.focusForeground": "#2D2A26",
    "quickInputList.focusIconForeground": "#C15F3C",
    "quickInputTitle.background": "#F4F3EE",
    "pickerGroup.border": "#D5D2CB",
    "pickerGroup.foreground": "#C15F3C",
    "button.background": "#C15F3C",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#A04D30",
    "scrollbarSlider.activeBackground": "#C15F3C80",
    "scrollbarSlider.background": "#B1ADA160",
    "scrollbarSlider.hoverBackground": "#9A958D80"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8A857D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#1E7B46"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#8B5CF6"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#8B5CF6"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C15F3C"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#6B5E50"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#2563EB"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#9333EA",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#0891B2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2D2A26"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#B45309",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#4D4943"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C15F3C"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#2563EB"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#5A5550"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#DC2626"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#C15F3C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#2D2A26",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#4D4943",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#2563EB"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#C15F3C"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#2563EB"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#2D2A26"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#2D2A26"
      }
    }
  ]
}
