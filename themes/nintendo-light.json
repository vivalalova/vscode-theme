{
  "name": "Lova Nintendo Light",
  "type": "light",
  "colors": {
    "activityBar.activeBorder": "#E60012",
    "activityBar.background": "#FAF9F8",
    "activityBar.foreground": "#E60012",
    "activityBar.inactiveForeground": "#6D6D6D",
    "activityBarBadge.background": "#E60012",
    "activityBarBadge.foreground": "#FFFFFF",
    "editor.background": "#FFFFFE",
    "editor.foreground": "#2D2D2D",
    "editor.lineHighlightBackground": "#FAF9F8",
    "editor.selectionBackground": "#E6001225",
    "editorCursor.foreground": "#E60012",
    "editorGroupHeader.tabsBackground": "#F5F4F3",
    "editorIndentGuide.activeBackground1": "#D5D5D5",
    "editorIndentGuide.background1": "#EAEAEA",
    "editorLineNumber.activeForeground": "#E60012",
    "editorLineNumber.foreground": "#8E8E8E",
    "focusBorder": "#E60012",
    "input.background": "#FFFFFE",
    "input.border": "#D5D5D5",
    "input.foreground": "#2D2D2D",
    "input.placeholderForeground": "#8E8E8E",
    "list.activeSelectionBackground": "#E6001218",
    "list.activeSelectionForeground": "#2D2D2D",
    "list.hoverBackground": "#F5F4F3",
    "panel.background": "#FAF9F8",
    "panel.border": "#D5D5D5",
    "panelTitle.activeForeground": "#E60012",
    "panelTitle.inactiveForeground": "#8E8E8E",
    "sideBar.background": "#FAF9F8",
    "sideBar.foreground": "#2D2D2D",
    "sideBarSectionHeader.background": "#EAEAEA",
    "sideBarSectionHeader.foreground": "#2D2D2D",
    "sideBarTitle.foreground": "#E60012",
    "statusBar.background": "#E60012",
    "statusBar.debuggingBackground": "#FF6B00",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.noFolderBackground": "#D5D5D5",
    "tab.activeBackground": "#FFFFFE",
    "tab.activeBorderTop": "#E60012",
    "tab.activeForeground": "#2D2D2D",
    "tab.inactiveBackground": "#F5F4F3",
    "tab.inactiveForeground": "#8E8E8E",
    "terminal.background": "#FFFFFE",
    "terminal.foreground": "#2D2D2D",
    "terminalCursor.foreground": "#E60012",
    "titleBar.activeBackground": "#F5F4F3",
    "titleBar.activeForeground": "#2D2D2D",
    "titleBar.inactiveBackground": "#F5F4F3",
    "titleBar.inactiveForeground": "#8E8E8E",
    "quickInput.background": "#FAF9F8",
    "quickInput.foreground": "#2D2D2D",
    "quickInputList.focusBackground": "#EAEAEA",
    "quickInputList.focusForeground": "#2D2D2D",
    "quickInputList.focusIconForeground": "#E60012",
    "quickInputTitle.background": "#FAF9F8",
    "pickerGroup.border": "#D5D5D5",
    "pickerGroup.foreground": "#E60012",
    "button.background": "#E60012",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#C8000F",
    "scrollbarSlider.activeBackground": "#E6001280",
    "scrollbarSlider.background": "#8E8E8E40",
    "scrollbarSlider.hoverBackground": "#8E8E8E60"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8E8E8E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#0D8050"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#0073CF"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#E60012"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#E60012"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#0073CF"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#7B4BBE",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#7B4BBE",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#0073CF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E60012"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7B4BBE"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#0073CF"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#E60012"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#E60012",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#0073CF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#0D8050",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#0073CF"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#0D8050"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "punctuation.support.type.property-name.begin.json", "punctuation.support.type.property-name.end.json"],
      "settings": {
        "foreground": "#7B4BBE"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#2D2D2D"
      }
    }
  ]
}
