  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#79747E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Comment",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#938F99",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Template String",
      "scope": ["string.template", "string.template.ts", "string.template.js"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#FFAB91"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Language Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#F2B8B5"
      }
    },
    {
      "name": "Character Constant",
      "scope": ["constant.character", "constant.character.escape"],
      "settings": {
        "foreground": "#FFAB91"
      }
    },
    {
      "name": "Other Constant",
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": {
        "foreground": "#E6E1E5"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#CE93D8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Language Variable",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#F2B8B5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant Variable",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.other"],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Control Keyword",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.flow"
      ],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Import Keyword",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.ternary",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#CAC4D0"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#90CAF9"
      }
    },
    {
      "name": "Function Name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#3DDC84"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call", "support.function"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Builtin Function",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Class Name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#A0F0C0",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Support Class",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Inherited Class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#A0F0C0",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Type Name",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#A0F0C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Primitive Type",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#90CAF9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Namespace",
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "entity.name.tag.html"],
      "settings": {
        "foreground": "#3DDC84"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Section",
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#3DDC84",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Support Constant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Support Variable",
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#CE93D8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.bracket"
      ],
      "settings": {
        "foreground": "#CAC4D0"
      }
    },
    {
      "name": "Punctuation Definition Tag",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#79747E"
      }
    },
    {
      "name": "Meta",
      "scope": [
        "meta.function",
        "meta.class",
        "meta.block",
        "meta.brace",
        "meta.bracket"
      ],
      "settings": {
        "foreground": "#E6E1E5"
      }
    },
    {
      "name": "Meta Return Type",
      "scope": ["meta.return.type"],
      "settings": {
        "foreground": "#A0F0C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type Annotation",
      "scope": ["meta.type.annotation"],
      "settings": {
        "foreground": "#A0F0C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#A0F0C0"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#E6E1E5"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#F2B8B5"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FFAB91",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "markup.heading.1", "markup.heading.2", "markup.heading.3"],
