  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#9B59B6"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#9B59B6"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2D7DD2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#E67E22"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#9B59B6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#1ABC9C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#2D7DD2"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#E67E22",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#2D7DD2"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#E74C3C"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#2D7DD2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#333333",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#555555",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#2D7DD2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Inline Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.raw.inline"],
      "settings": {
        "foreground": "#E67E22"
      }
    },
    {
      "name": "Markup Code Block",
      "scope": ["markup.fenced_code.block.markdown", "markup.raw.block"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#2D7DD2"
      }
    },
    {
      "name": "JSON String Value",
      "scope": ["meta.structure.dictionary.value.json string.quoted.double.json"],
      "settings": {
        "foreground": "#27AE60"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#2D7DD2"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#9B59B6"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#E67E22"
      }
    },
    {
      "name": "Swift Source",
      "scope": ["source.swift"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#E67E22"
      }
    },
    {
      "name": "Escape Character",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "name": "Template Expression",
      "scope": ["punctuation.definition.template-expression", "meta.template.expression"],
      "settings": {
        "foreground": "#2D7DD2"
      }
    }
  ]
}
