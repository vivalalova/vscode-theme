    "activityBarBadge.background": "#18181B",
    "activityBarBadge.foreground": "#FAFAFA",
    "editor.background": "#FAFAFA",
    "editor.foreground": "#09090B",
    "editor.lineHighlightBackground": "#F4F4F5",
    "editor.selectionBackground": "#18181B25",
    "editorCursor.foreground": "#18181B",
    "editorGroupHeader.tabsBackground": "#F4F4F5",
    "editorIndentGuide.activeBackground1": "#A1A1AA",
    "editorIndentGuide.background1": "#E4E4E7",
    "editorLineNumber.activeForeground": "#18181B",
    "editorLineNumber.foreground": "#A1A1AA",
    "focusBorder": "#18181B",
    "input.background": "#FFFFFF",
    "input.border": "#E4E4E7",
    "input.foreground": "#09090B",
    "input.placeholderForeground": "#71717A",
    "list.activeSelectionBackground": "#18181B15",
    "list.activeSelectionForeground": "#09090B",
    "list.hoverBackground": "#F4F4F5",
    "panel.background": "#FAFAFA",
    "panel.border": "#E4E4E7",
    "panelTitle.activeForeground": "#18181B",
    "panelTitle.inactiveForeground": "#71717A",
    "sideBar.background": "#F4F4F5",
    "sideBar.foreground": "#18181B",
    "sideBarSectionHeader.background": "#E4E4E7",
    "sideBarSectionHeader.foreground": "#18181B",
    "sideBarTitle.foreground": "#09090B",
    "statusBar.background": "#18181B",
    "statusBar.debuggingBackground": "#09090B",
    "statusBar.debuggingForeground": "#FAFAFA",
    "statusBar.foreground": "#FAFAFA",
    "statusBar.noFolderBackground": "#E4E4E7",
    "tab.activeBackground": "#FAFAFA",
    "tab.activeBorderTop": "#18181B",
    "tab.activeForeground": "#09090B",
    "tab.inactiveBackground": "#F4F4F5",
    "tab.inactiveForeground": "#52525B",
    "terminal.background": "#FAFAFA",
    "terminal.foreground": "#09090B",
    "terminalCursor.foreground": "#18181B",
    "titleBar.activeBackground": "#F4F4F5",
    "titleBar.activeForeground": "#18181B",
    "titleBar.inactiveBackground": "#F4F4F5",
