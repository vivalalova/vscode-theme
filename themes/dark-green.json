{
  "name": "Lova Monokai Green",
  "type": "dark",
  "colors": {
    "activityBar.activeBorder": "#4EC9B0",
    "activityBar.background": "#383838",
    "activityBar.foreground": "#4EC9B0",
    "activityBar.inactiveForeground": "#999999",
    "activityBarBadge.background": "#4EC9B0",
    "activityBarBadge.foreground": "#1e1e1e",
    "editor.background": "#2f2f2f",
    "editorGroupHeader.tabsBackground": "#3a3a3a",
    "panel.background": "#2f2f2f",
    "panel.border": "#454545",
    "panelTitle.activeForeground": "#4EC9B0",
    "panelTitle.inactiveForeground": "#999999",
    "sideBar.background": "#353535",
    "sideBarSectionHeader.background": "#3d3d3d",
    "sideBarSectionHeader.foreground": "#cccccc",
    "sideBarTitle.foreground": "#4EC9B0",
    "statusBar.background": "#1a5045",
    "statusBar.debuggingBackground": "#cc6633",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ffffff",
    "tab.activeBackground": "#2f2f2f",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#3d3d3d",
    "tab.inactiveForeground": "#999999",
    "terminal.background": "#2f2f2f",
    "terminal.foreground": "#cccccc",
    "terminalCursor.foreground": "#4EC9B0",
    "titleBar.activeBackground": "#454545",
    "titleBar.activeForeground": "#cccccc",
    "titleBar.inactiveBackground": "#454545",
    "titleBar.inactiveForeground": "#999999",
    "quickInput.background": "#2f2f2f",
    "quickInput.foreground": "#cccccc",
    "quickInputList.focusBackground": "#3d3d3d",
    "quickInputList.focusForeground": "#ffffff",
    "quickInputList.focusIconForeground": "#4EC9B0",
    "quickInputTitle.background": "#2f2f2f",
    "pickerGroup.border": "#454545",
    "pickerGroup.foreground": "#4EC9B0"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#75715E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FD971F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#F8F8F2",
        "background": "#F92672"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#E6DB74",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "HTML Text",
      "scope": ["text.html"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    }
  ]
}
