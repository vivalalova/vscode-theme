  "semanticTokenColors": {
    "variable": "#FFFFFF",
    "variable.readonly": "#FFFFFF",
    "variable.local": "#FFFFFF",
    "variable.declaration": "#FFFFFF",
    "property": "#FFFFFF",
    "parameter": "#e6c070"
  },
  "colors": {
    "activityBar.activeBorder": "#a490c2",
    "activityBar.background": "#2b1e3e",
