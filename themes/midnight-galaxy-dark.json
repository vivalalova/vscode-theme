    "editorCursor.foreground": "#a490c2",
    "editorGroupHeader.tabsBackground": "#221833",
    "editorLineNumber.foreground": "#5a4d6a",
    "editorLineNumber.activeForeground": "#a490c2",
    "editorIndentGuide.background": "#3d3050",
    "editorIndentGuide.activeBackground": "#4d4060",
    "editorBracketMatch.background": "#4a4e8f40",
    "editorBracketMatch.border": "#a490c2",
    "panel.background": "#221833",
    "panel.border": "#3d3050",
    "panelTitle.activeForeground": "#a490c2",
    "panelTitle.activeBorder": "#4a4e8f",
    "panelTitle.inactiveForeground": "#6b5d7a",
    "panelSectionHeader.background": "#2b1e3e",
    "sideBar.background": "#221833",
    "sideBar.border": "#3d3050",
    "sideBar.foreground": "#c0b8d0",
    "sideBarSectionHeader.background": "#2b1e3e",
    "sideBarSectionHeader.foreground": "#e6e6fa",
    "sideBarTitle.foreground": "#4a4e8f",
    "statusBar.background": "#4a4e8f",
    "statusBar.border": "#3d3f75",
    "statusBar.foreground": "#e6e6fa",
    "statusBar.debuggingBackground": "#d47070",
    "statusBar.debuggingForeground": "#e6e6fa",
    "statusBar.noFolderBackground": "#2b1e3e",
    "tab.activeBackground": "#2b1e3e",
    "tab.activeForeground": "#e6e6fa",
    "tab.activeBorderTop": "#a490c2",
    "tab.inactiveBackground": "#221833",
    "tab.inactiveForeground": "#6b5d7a",
    "tab.border": "#22183380",
    "terminal.background": "#2b1e3e",
    "terminal.foreground": "#e6e6fa",
    "terminalCursor.foreground": "#a490c2",
    "terminal.ansiBlack": "#2b1e3e",
    "terminal.ansiRed": "#d47070",
    "terminal.ansiGreen": "#7acc7a",
    "terminal.ansiYellow": "#e6c070",
    "terminal.ansiBlue": "#4a4e8f",
    "terminal.ansiMagenta": "#a490c2",
    "terminal.ansiCyan": "#70acc0",
    "terminal.ansiWhite": "#e6e6fa",
    "titleBar.activeBackground": "#221833",
    "titleBar.activeForeground": "#e6e6fa",
    "titleBar.inactiveBackground": "#221833",
    "titleBar.inactiveForeground": "#6b5d7a",
    "titleBar.border": "#3d3050",
    "quickInput.background": "#2b1e3e",
    "quickInput.foreground": "#e6e6fa",
    "quickInputList.focusBackground": "#4a4e8f40",
    "quickInputList.focusForeground": "#e6e6fa",
    "quickInputList.focusIconForeground": "#a490c2",
    "quickInputTitle.background": "#2b1e3e",
    "pickerGroup.border": "#3d3050",
    "pickerGroup.foreground": "#4a4e8f",
    "list.activeSelectionBackground": "#4a4e8f40",
    "list.activeSelectionForeground": "#e6e6fa",
    "list.hoverBackground": "#332848",
    "list.inactiveSelectionBackground": "#3d3050",
    "scrollbarSlider.background": "#4a4e8f40",
    "scrollbarSlider.hoverBackground": "#4a4e8f60",
    "scrollbarSlider.activeBackground": "#4a4e8f80",
    "input.background": "#221833",
    "input.border": "#3d3050",
    "input.foreground": "#e6e6fa",
    "input.placeholderForeground": "#6b5d7a",
    "inputOption.activeBorder": "#a490c2",
    "button.background": "#4a4e8f",
    "button.foreground": "#e6e6fa",
